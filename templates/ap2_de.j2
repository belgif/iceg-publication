<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
{% set language = "de" %}
{% set sortattr = "sort" %}

<html xml:lang="de" lang="de" dir="ltr" typeof="bibo:Document " about="" property="dcterms:language" content="de" prefix="dcterms: http://purl.org/dc/terms/ bibo: http://purl.org/ontology/bibo/ schema: http://schema.org/ w3p: http://www.w3.org/2001/02pd/rec54#" xmlns="http://www.w3.org/1999/xhtml">

<head>
  {% set name = metadata.title|lower|replace(' ', '-') %}
  <title>{{ metadata.title }}</title>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <link rel="stylesheet" href="https://dij151upo6vad.cloudfront.net/2.latest/css/vlaanderen-ui.css" />
  <link rel="stylesheet" href="https://dij151upo6vad.cloudfront.net/2.latest/css/vlaanderen-ui-corporate.css" />
  <link rel="stylesheet" href="https://code.jquery.com/ui/1.13.2/themes/base/jquery-ui.css" />

  <link rel="icon" sizes="192x192" href="/cms/icons/DUETLogo.jpg" />
  <link rel="apple-touch-icon" href="/cms/icons/DUETLogo.jpg" />
  <link rel="apple-touch-icon" sizes="76x76" href="/cms/icons/DUETLogo.jpg" />
  <link rel="apple-touch-icon" sizes="120x120" href="/cms/icons/DUETLogo.jpg" />
  <link rel="apple-touch-icon" sizes="152x152" href="/cms/icons/DUETLogo.jpg" />

  <meta name="msapplication-square70x70logo" content="/cms/icons/DUETLogo.jpg" />
  <meta name="msapplication-square150x150logo" content="/cms/icons/DUETLogo.jpg" />
  <meta name="msapplication-wide310x150logo" content="/cms/icons/DUETLogo.jpg" />
  <meta name="msapplication-square310x310logo" content="/cms/icons/DUETLogo.jpg" />
  <meta name="msapplication-TileColor" content="#7ED956" />
</head>

<body>

  <script type="text/javascript">
    function iframeLoaded() {
      var iFrameID = document.getElementById('exampleiframe');
      if (iFrameID) {
        // here you can make the height, I delete it first, then I make it again
        iFrameID.height = "";
        iFrameID.height = iFrameID.contentWindow.document.body.scrollHeight + "px";
      }
    }
  </script>

  <div style="min-height:44px;;" id="duet-header" class="header">
    <!-- insert your global header here -->
    <title>Belgif</title>
    <i class="logo">Belgif logo</i>
    <!-- end global header-->
  </div>

  <!-- Start page content -->
  <div class="page">
    <div class="layout layout--wide">
      <div class="skiplink">
        <a rel="nofollow noreferrer" href="#main">Überspringen Sie diesen Abschnitt und gehen Sie zum allgemeinen Inhalt.</a>
      </div>
    </div>
    <div class="page">
      <div class="region">
        <div class="layout layout--wide">
          <div class="grid u-hr">
            <div class="col--9-12 col--1-1--s">
              <h1 class="h1">{{ metadata.title }}</h1>

              <div class="head" role="status">
                <dl>
                  <dt>Status</dt>
                  <dd><a href="{{metadata.status}}">{{metadata.status}}</a></dd>
                  <dt>Veröffentlicht am</dt>
                  <dd>{{metadata.issued}}</dd>
                  <dt>Diese Version</dt>
                  <dd><a href="{{metadata.navigation.self}}">{{metadata.navigation.self}}</a></dd>
                  {% if metadata.navigation.next %}
                  <dt>Nächste Version</dt>
                  <dd><a href="{{metadata.navigation.next}}">{{metadata.navigation.next}}</a></dd>
                  {% endif %}
                  {% if metadata.navigation.prev %}
                  <dt>Vorherige Version</dt>
                  <dd><a href="{{metadata.navigation.prev}}">{{metadata.navigation.prev}}</a></dd>
                  {% endif %}
                </dl>
              </div>

              <div class="head" role="contentinfo" id="respecHeader">
                <dl>
                  {% for role, contributors in contributors | groupby("role") %}
                  {% if role == "A" %}
                  {% set authors = contributors|sort(attribute='last_name') %}
                  {% if authors %}
                  <dt>Autoren</dt>
                  {% for author in authors %}
                  <dd inlist="" rel="foaf:maker" class="p-author h-card vcard">
                    <span typeof="foaf:Person">
                      <span class="p-name fn" property="foaf:lastName">{{ author.last_name }}</span>,
                      <span class="p-name fn" property="foaf:firstName">{{ author.first_name }}</span>
                      {% if author.affiliation.website and author.affiliation.name %}
                      - <a href="{{ author.affiliation.website }}" class="p-org org h-org h-card" rel="foaf:workplaceHomepage">{{ author.affiliation.name }}</a>
                      {% elif author.affiliation.name %}
                      - <span class="p-org org h-org h-card">{{ author.affiliation.name }}</span>
                      {% endif %}
                      {% if author.email %}
                      - <span class="ed_mailto"><a href="mailto:{{ author.email }}" rel="foaf:mbox" class="u-email email">{{ author.email }}</a></span>
                      {% endif %}
                    </span>
                  </dd>
                  {% endfor %}
                  {% endif %}
                  {% elif role == "E" %}
                  {% set editors = contributors|sort(attribute='last_name') %}
                  {% if editors %}
                  <dt>Redakteure</dt>
                  {% for editor in editors %}
                  <dd inlist="" rel="bibo:editor" class="p-author h-card vcard">
                    <span typeof="foaf:Person">
                      <span class="p-name fn" property="foaf:lastName">{{ editor.last_name }}</span>,
                      <span class="p-name fn" property="foaf:firstName">{{ editor.first_name }}</span>
                      {% if editor.affiliation.website and editor.affiliation.name %}
                      - <a href="{{ editor.affiliation.website }}" class="p-org org h-org h-card" rel="foaf:workplaceHomepage">{{ editor.affiliation.name }}</a>
                      {% elif editor.affiliation.name %}
                      - <span class="p-org org h-org h-card">{{ editor.affiliation.name }}</span>
                      {% endif %}
                      {% if editor.email %}
                      - <span class="ed_mailto"><a href="mailto:{{ editor.email }}" rel="foaf:mbox" class="u-email email">{{ editor.email }}</a></span>
                      {% endif %}
                    </span>
                  </dd>
                  {% endfor %}
                  {% endif %}
                  {% elif role == "C" %}
                  {% set pure_contributors = contributors|sort(attribute='last_name') %}
                  {% if pure_contributors %}
                  <dt>Beitragende</dt>
                  {% for contributor in pure_contributors %}
                  <dd inlist="" rel="dcterms:contributor" class="p-author h-card vcard">
                    <span typeof="foaf:Person">
                      <span class="p-name fn" property="foaf:lastName">{{ contributor.last_name }}</span>,
                      <span class="p-name fn" property="foaf:firstName">{{ contributor.first_name }}</span>
                      {% if contributor.affiliation.website and contributor.affiliation.name %}
                      - <a href="{{ contributor.affiliation.website }}" class="p-org org h-org h-card" rel="foaf:workplaceHomepage">{{ contributor.affiliation.name }}</a>
                      {% elif contributor.affiliation.name %}
                      - <span class="p-org org h-org h-card">{{ contributor.affiliation.name }}</span>
                      {% endif %}
                      {% if contributor.email %}
                      - <span class="ed_mailto"><a href="mailto:{{ contributor.email }}" rel="foaf:mbox" class="u-email email">{{ contributor.email }}</a></span>
                      {% endif %}
                    </span>
                  </dd>
                  {% endfor %}
                  {% endif %}
                  {% endif %}
                  {% endfor %}
                </dl>
              </div>

              <div class="head" role="metainfo" id="metainfo">
                <dl>
                  <dt>Quelle</dt>
                  <dd><a href="{{metadata.repositoryurl}}">{{metadata.repositoryurl}}</a></dd>
                  <dt>Changelog</dt>
                  <dd><a href="{{metadata.changelogurl}}">{{metadata.changelogurl}}</a></dd>
                  <dt>Feedback</dt>
                  <dd><a href="{{metadata.feedbackurl}}">{{metadata.feedbackurl}}</a></dd>
                  {# <dt>Spezifikationsregister</dt>
                  <dd><a href="{{metadata.standaardregisterurl}}">{{metadata.standaardregisterurl}}</a></dd> #}

                  {# <dt>Abhängigkeiten</dt>
                  <dd>
                    {% for dep in metadata.dependencies %}
                    <a href="{{ dep.packageurl }}">{{ dep.packagelabel }}</a>
                    {% endfor %}
                  </dd> #}
                </dl>
              </div>

            </div>
          </div>
        </div>
      </div>
    </div>

    <div id="main" itemprop="mainContentOfPage" role="main" tabindex="-1" class="main-content">
      <div class="layout layout--wide">
        <div class="grid">
          <div class="col--8-12 col--12-12--s">

            <div class="region region--no-space-top typography" id="summary">
              <h2 class="h2" aria-level="1" role="heading" id="h2_sotd">Zusammenfassung</h2>
              {% block summary %}
              {% if metadata.description %}
              {{ metadata.description[language] }}
              {% endif %}
              {% endblock %}
            </div>

            <div class="region region--no-space-top typography" id="status">
              <section class="introductory" id="sotd" typeof="bibo:Chapter" resource="#ref" rel="bibo:Chapter">
                <h2 class="h2" aria-level="1" role="heading" id="h2_sotd">Status dieses Dokuments</h2>
                {% block status %}
                <p>
                  Diese Anwendungsprofil hat den Status <a href="{{metadata.status }}">{{ metadata.statuslabel }}</a> und wurde am {{ metadata.issued }} veröffentlicht.
                </p>
                {# <p>
                  Informationen über den Prozess und die getroffenen Entscheidungen bei der Erstellung dieser Spezifikation sind unter dem <a href="{{metadata.standaardregisterurl}}">Spezifikationsregister</a> abrufbar.
                </p> #}
                {% endblock %}
              </section>
            </div>

            {# <div class="region" id="introduction">
              <div class="introduction typography">
                {% block introduction %}
                <p>Dieses Dokument beschreibt ein <strong>Anwendungsprofil</strong>, in diesem Fall <strong>{{ metadata.title }}</strong>.
                Das Anwendungsprofil legt ein semantisches Datenmodell für eine Reihe von Anwendungsfällen fest.
                Die Spezifikation besteht aus Begriffen mit ihren Einschränkungen (Kardinalitäten, Codelists, Verwendungshinweise).
                </p>
                {% endblock %}
              </div>
            </div> #}

            <div class="region region--no-space-top typography" id="conformance">
              <h2 class="h2" id="conformance" aria-level="1" role="heading">Konformität</h2>
              {% block conformance %}
              <p>To be defined</p>
              {% endblock %}
            </div>

            {# <div class="region region--no-space-top typography" id="license">
              <h2 class="h2">Lizenz</h2>
              {% block license %}
              <p><span rel="dct:type" property="dct:title" href="http://purl.org/dc/dcmitype/Text" xmlns:dct="http://purl.org/dc/terms/">Diese Spezifikation</span> von <a rel="cc:attributionURL" property="cc:attributionName" href="ORGANISATION" xmlns:cc="http://creativecommons.org/ns#">ORGANISATION</a> wird unter <a href="{{ metadata.license }}" rel="license">"CC-BY 4.0"</a> veröffentlicht.</p>
              {% endblock %}
            </div> #}

            <div class="region region--no-space-top" id="overview">
              <h2 class="h2">Übersicht</h2>
              <div class="grid">
                <div class="col--9-12 col--1-1--s typography">
                  {% if ( ( classes | length > 0) ) %}
                  <div class="region region--no-space-top">
                    <p>Dieses Dokument beschreibt die Verwendung der folgenden Entitäten für eine korrekte Anwendung des Anwendungsprofils:
                      <br>
                      |
                      {%- for class in classes |sort(attribute=sortattr) %}
                      <a href="#{{ class.name }}">{{ class.label[language] }}</a> |
                      {%- endfor %}
                    </p>
                  </div>
                  {% endif %}
                  {% if ( ( datatypes | length ) > 0 ) %}
                  <div class="region region--no-space-top">
                    <p>Dieses Dokument beschreibt die Verwendung der folgenden Datentypen für eine korrekte Anwendung des Anwendungsprofils:
                      <br>
                      |
                      {%- for datatype in datatypes |sort(attribute=sortattr) %}
                      <a href="#{{ datatype.name }}">{{ datatype.label[language] }}</a> |
                      {%- endfor %}
                    </p>
                  </div>
                  {% endif %}
                </div>
              </div>
            </div>

            <div class="region region--no-space-top">
              {% block images %}
              <a href="{{ documentroot }}/html/overview.jpg" target="_blank">
                <img src="{{ documentroot }}/html/overview.jpg" />
              </a>
              {% endblock %}
            </div>
            <div class="region region--no-space-top">
              <h2 class="h2">Entitäten</h2>
            </div>
            {% for entity in classes | sort(attribute=sortattr) %}
            <div class="region region--no-space-top">
              <h3 class="h3" id="{{ entity.name }}"><a href="{{ entity.uri }}" data-toggle="tooltip" data-content="{{ entity.uri }}" data-placement="right">{{ entity.label[language] }}</a></h3>
              <div class="region region--no-space-top">
                <dl>
                  <dt>Definition</dt>
                  <dd>{{ entity.description[language] }}</dd>
                  {% if entity.usage[language] %}
                  <dt>Verwendung</dt>
                  <dd>{{ entity.usage[language] | safe }}</dd>
                  {% endif %}
                  {% if ((entity.parents | length) > 0 ) %}
                  <dt>Unterklasse von
                  <dt>
                  <dd>
                    {%- for parent in entity['parents'] %}
                    <a href="{{ parent.scoped_uri}}">{{ parent.label }}</a>{% if not loop.last %}, {% endif %}
                    {%- endfor %}
                  </dd>
                  {% endif %}
                  <dt>Eigenschaften</dt>
                  {% if ((entity.properties | length) > 0 ) %}
                  <dd>Für diese Entität sind die folgenden Eigenschaften definiert:
                    {%- for prop in entity['properties'] | sort(attribute=sortattr) %}
                    <a href="#{{ (entity.name ~ ':' ~ prop.name) | urlencode }}">{{ prop.label[language] }}</a>{% if not loop.last %}, {% endif %}
                    {%- endfor %}.
                  </dd>
                  {% else %}
                  <dd>Für diese Entität wurden keine Eigenschaften definiert.</dd>
                  {% endif %}
                </dl>
              </div>
              {% if ((entity.properties | length) > 0 ) %}
              <div class="u-table-overflow">
                <table class="data-table">
                  <thead>
                    <tr class="data-table__header">
                      <th class="data-table__header-title--sortable data-table__header-title--sortable-active">Eigenschaft</th>
                      <th class="data-table__header-title--sortable">Erwarteter Bereich</th>
                      <th class="data-table__header-title--sortable">Kardinalität</th>
                      <th class="data-table__header-title--sortable">Definition</th>
                      <th class="data-table__header-title--sortable">Verwendung</th>
                      <th class="data-table__header-title--sortable">Codelist</th>
                    </tr>
                  </thead>

                  <tbody class="supertype">
                    {% for prop in entity['properties'] | sort(attribute=sortattr) %}
                    <tr id="{{ (entity.name ~ ':' ~ prop.name) | urlencode }}" typeof="rdfs:Property" resource="{{ prop.uri }}">
                      <td>
                        <code property="rdfs:label">
                            <a href="{{ prop.uri }}" data-toggle="tooltip" data-content="{{ prop.uri }}" data-placement="right">
                            {{ prop.label[language] }}</a>
                          </code>
                      </td>
                      <td>
                        {% for range in prop.scopedrange %}
                        {% if range['range_uri'] == range['range_puri'] %}
                        <a href="{{ range['range_uri'] }}" data-toggle="tooltip" data-content="{{ range['range_puri'] }}" data-placement="right">{{ range['range_label'][language] }}</a>
                        {% else %}
                        <a href="{{ range['range_uri'] }}" data-placement="right">{{ range['range_label'][language] }}</a>
                        {% endif %}
                        {% endfor %}
                      </td>
                      <td>{{ prop.cardinality }}</td>
                      <td property="rdfs:comment">{{ prop.description[language] }}</td>
                      <td property="skos:scopeNote">{{ prop.usage[language] | safe }}</td>
                      <td>{% if prop['codelist_uri'] %}<a href="{{ prop['codelist_uri'] }}">Link</a>{% endif %}</td>
                    </tr>
                    {% endfor %}
                  </tbody>
                </table>
              </div>
              {% endif %}
            </div>
            {% endfor %}

            {% if  (( datatypes | length ) > 0 ) %}
            <div class="region region--no-space-top">
              <h2 class="h2">Datentypen</h2>
            </div>
            {% for entity in datatypes | sort(attribute=sortattr)%}
            <div class="region region--no-space-top">
              {% if entity.scopeduri %}
              <h3 class="h3" id="{{ entity.name }}"><a href="{{ entity.scopeduri }}" data-toggle="tooltip" data-content="{{ entity.uri }}" data-placement="right">{{ entity.label[language] }}</a></h3>
              {% else %}
              <h3 class="h3" id="{{ entity.name }}"><a href="{{ entity.uri }}" data-toggle="tooltip" data-content="{{ entity.uri }}" data-placement="right">{{ entity.label[language] }}</a></h3>
              {% endif %}
              <div class="region region--no-space-top">
                <dl>
                  <dt>Definition</dt>
                  <dd>{{ entity.description[language] }}</dd>
                  {% if entity.usage[language] %}
                  <dt>Verwendung</dt>
                  <dd>{{ entity.usage[language] | safe }}</dd>
                  {% endif %}
                  {% if ((entity.parents | length) > 0 ) %}
                  <dt>Unterklasse von
                  <dt>
                  <dd>
                    {%- for parent in entity['parents'] %}
                    <a href="{{ parent.scoped_uri}}">{{ parent.label }}</a>{% if not loop.last %}, {% endif %}
                    {%- endfor %}
                  </dd>
                  {% endif %}
                  <dt>Eigenschaften</dt>
                  {% if ((entity.properties | length) > 0 ) %}
                  <dd>Für diesen Datentyp sind die folgenden Eigenschaften definiert:
                    {%- for prop in entity['properties'] | sort(attribute=sortattr) %}
                    <a href="#{{ (entity.name ~ ':' ~ prop.name) | urlencode }}">{{ prop.label[language] }}</a>{% if not loop.last %}, {% endif %}
                    {%- endfor %}.
                  </dd>
                  {% else %}
                  <dd>Für diesen Datentyp wurden keine Eigenschaften definiert.</dd>
                  {% endif %}
                </dl>
              </div>
              {% if ((entity.properties | length) > 0 ) %}
              <div class="u-table-overflow">
                <table class="data-table">
                  <thead>
                    <tr class="data-table__header">
                      <th class="data-table__header-title--sortable data-table__header-title--sortable-active">Eigenschaft</th>
                      <th class="data-table__header-title--sortable">Erwarteter Bereich</th>
                      <th class="data-table__header-title--sortable">Kardinalität</th>
                      <th class="data-table__header-title--sortable">Definition</th>
                      <th class="data-table__header-title--sortable">Verwendung</th>
                      <th class="data-table__header-title--sortable">Codelist</th>
                    </tr>
                  </thead>

                  <tbody class="supertype">
                    {% for prop in entity['properties'] | sort(attribute=sortattr) %}
                    <tr id="{{ (entity.name ~ ':' ~ prop.name ) | urlencode }}" typeof="rdfs:Property" resource="{{ prop.uri }}">
                      <td>
                        <code property="rdfs:label">
                            <a href="{{ prop.uri }}" data-toggle="tooltip" data-content="{{ prop.uri }}" data-placement="right">{{ prop.label[language] }}</a>
                          </code>
                      </td>
                      <td>
                        {% for range in prop.scopedrange %}
                        {% if range['range_uri'] == range['range_puri'] %}
                        <a href="{{ range['range_uri'] }}" data-toggle="tooltip" data-content="{{ range['range_puri'] }}" data-placement="right">{{ range['range_label'][language] }}</a>
                        {% else %}
                        <a href="{{ range['range_uri'] }}" data-placement="right">{{ range['range_label'][language] }}</a>
                        {% endif %}
                        {% endfor %}
                      </td>
                      <td>{{ prop['cardinality'] }}</td>
                      <td property="rdfs:comment">{{ prop.description[language] }}</td>
                      <td property="skos:scopeNote">{{ prop.usage[language] | safe }}</td>
                      <td>{% if prop['codelist_uri'] %}<a href="{{ prop['codelist_uri'] }}">Link</a>{% endif %}</td>
                    </tr>
                    {% endfor %}
                  </tbody>
                </table>
              </div>
              {% endif %}
            </div>
            {% endfor %}
            {% endif %}
            <div class="region region--no-space-top" id="codelists">
              <h2 class="h2" aria-level="1" role="heading" id="h2_sotd">Listen</h2>
              (nicht normativ)

              {% block codelist %}
              {% endblock %}
            </div>

            <div class="region region--no-space-top" id="changelog">
              <h2 class="h2" aria-level="1" role="heading" id="h2_sotd">Changelog</h2>
              (nicht normativ)
              {% block changelog %}
              <br>
              <p>placeholder</p>
              {% endblock %}
            </div>
            <div class="region region--no-space-top" id="jsonld">
              <h2 class="h2" aria-level="1" role="heading" id="h2_sotd">JSON-LD context</h2>
              (nicht normativ)
              {% block jsonld %}
              <p>Eine wiederverwendbare JSON-LD-Kontextdefinition für dieses Anwendungsprofil ist abrufbar unter: <a href="{{ documentroot }}/context/{{ metadata.filename }}.jsonld" target="_blank">{{ documentroot }}/context/{{ metadata.filename }}.jsonld</a></p>
              {% endblock %}
            </div>
            <div class="region region--no-space-top" id="shacl">
              <h2 class="h2" aria-level="1" role="heading" id="h2_sotd">SHACL template</h2>
              (nicht normativ)
              {% block shacl%}
              <p>Eine wiederverwendbare SHACL-Vorlage für dieses Anwendungsprofil ist abrufbar unter: <a href="{{ documentroot }}/shacl/{{ metadata.filename }}-SHACL.ttl" target="_blank">{{ documentroot }}/shacl/{{ metadata.filename }}-SHACL.ttl</a></p>
              {% endblock %}
            </div>
          </div>
          <div class="col--3-12 col--12-12--s col--4-12--m push--1-12 push--reset--m" id="sideNav">
            <nav class="side-navigation js-sticky region" id="sideNav">
              <div class="side-navigation__content js-scrollspy">
                <div class="side-navigation__group">
                  <ul>
                    {# <li class="side-navigation__item">
                      <a href="#introduction">Einleitung</a>
                    </li> #}
                    <li class="side-navigation__item">
                      <a href="#summary">Zusammenfassung</a>
                    </li>
                    <li class="side-navigation__item">
                      <a href="#status">Status dieses Dokuments</a>
                    </li>
                    {# <li class="side-navigation__item">
                      <a href="#license">Lizenz</a>
                    </li> #}
                    <li class="side-navigation__item">
                      <a href="#conformance">Konformität</a>
                    </li>
                    <li class="side-navigation__item">
                      <a href="#overview">Übersicht</a>
                    </li>
                    <li class="side-navigation__item">
                      <a href="#codelists">Listen</a>
                    </li>
                    <li class="side-navigation__item">
                      <a href="#changelog">Changelog</a>
                    </li>
                    <li class="side-navigation__item">
                      <a href="#jsonld">JSON-LD context</a>
                    </li>
                    <li class="side-navigation__item">
                      <a href="#shacl">SHACL template</a>
                    </li>
                  </ul>
                </div>
              </div>
            </nav>
          </div>
          <!-- End page content -->
        </div>
      </div>
    </div>
  </div>

  <script src="https://dij151upo6vad.cloudfront.net/2.latest/js/vlaanderen-ui.js"></script>
  <script type="text/javascript">
    function iframeLoaded() {
      var iFrameID = document.getElementById('exampleiframe');
      if (iFrameID) {
        // here you can make the height, I delete it first, then I make it again
        iFrameID.height = "";
        iFrameID.height = iFrameID.contentWindow.document.body.scrollHeight + "px";
      }
    }
  </script>
  <script src="https://code.jquery.com/jquery-3.6.0.js"></script>
  <script src="https://code.jquery.com/ui/1.13.2/jquery-ui.js"></script>
  <script>
    $(function () {
      $("#tabs").tabs();
    });
  </script>

  <style>
    @media screen {

      /* hide from IE3 */
      a[href]:hover {
        background: #ffa
      }
    }

    p {
      text-align: justify;
    }

    tr:nth-child(even) td {
      background-color: #eee;
    }

    dt,
    dd {
      margin-top: 0;
      margin-bottom: 0
    }

